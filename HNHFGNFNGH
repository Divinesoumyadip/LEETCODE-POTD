#include <bits/stdc++.h>
#include <math.h>
#define name " "

using namespace std;
using ull = unsigned long long;
const long long MAX = 1e6 + 2;

long long t;

int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0); cout.tie(0);

    if(fopen(name".INP", "r")){
       freopen(name".INP", "r", stdin);
       freopen(name".OUT", "w", stdout);
    }
    cin >> t;
    while(t--){
        long long n;
        cin >> n;
        long long cnt = 0;
        bool check = true;
        vector <long long> a;
        for(int i = 1; i <= n; i++){
            cnt += i;
            long long k = int(sqrt(cnt));
            if(k * k == cnt){
                if(i == n) check = false;
                else{
                    a.push_back(i + 1);
                    a.push_back(i);
                    cnt += i + 1;
                    i++;
                }
            }
            else a.push_back(i);
        }
        if(check){
            long long cnt2 = 0;
            for(int i = 0; i < a.size(); i++){
                cnt2 += a[i];
                long long k = sqrt(cnt2);
                //if(k * k == cnt2) cout << i;
                cout << a[i] << " ";
            }
        }
        else cout << -1;
        cout << '\n';
    }
}




